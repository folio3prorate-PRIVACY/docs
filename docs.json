{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "Octav Developer Docs",
  "version": "2.1.0",
  "colors": {
    "primary": "#3F58C7",
    "light": "#3F58C7",
    "dark": "#3F58C7"
  },
  "navbar": {
    "links": [
      {
        "label": "Get Help",
        "href": "https://discord.gg/qvcknAz73A",
        "icon": "discord",
        "iconType": "solid"
      },
      {
        "label": "Dev Portal",
        "href": "https://data.octav.fi/",
        "icon": "/logo/Octav_logo_icon_red@2x.png"
      }
    ]
  },
  "font": {
    "headings": {
      "family": "Space Grotesk"
    },
    "body": {
      "family": "Geist"
    },
    "code": {
      "family": "Geist Mono"
    }
  },
  "logo": {
    "light": "/logo/logo-dark.svg",
    "dark": "/logo/logo-light.svg",
    "href": "https://octav.fi"
  },
  "favicon": "/logo/Octav_logo_icon_red@2x.png",
  "customCSS": [
    "/custom.css"
  ],
  "metadata": {
    "og:image": "https://octav.fi/og-image.png"
  },
  "footerSocials": {},
  "api": {
    "baseUrl": "https://api.octav.fi/v1",
